{
  "name": "@zoian/app",
  "version": "0.0.0",
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@vueuse/core": "^12.0.0",
    "core-js": "^3.37.1",
    "debug": "^4.4.0",
    "jszip": "^3.10.1",
    "pinia": "^2.1.7",
    "rambdax": "^11.3.0",
    "vue": "^3.4.31",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "d3-interpolate": "^3.0.1",
    "@types/d3-interpolate": "^3.0.4",
    "@eslint/js": "^9.14.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "json-stringify-pretty-compact": "^4.0.0",
    "npm-run-all2": "^7.0.1",
    "sass": "1.77.8",
    "sass-embedded": "^1.77.8",
    "typescript": "~5.6.3",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.4.10",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.1.10"
  },
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-gh-pages": "deno run -A npm:vite build --out-dir=docs",
    "build-only": "deno run -A npm:vite build ",
    "deno:install": "deno install --allow-scripts",
    "dev": "vite",
    "lint:app": "deno run -A npm:eslint . --fix",
    "lint:lib": "cd lib && deno lint --fix --compact && deno fmt",
    "preview": "deno run -A npm:vite preview",
    "test:lib": "cd lib && deno task test",
    "type-check": "vue-tsc --build --force"
  },
  "type": "module"
}
